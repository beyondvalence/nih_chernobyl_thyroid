!!! All

select dgy<4@
freq thnod_prev @


!Basic linear dose model 
nomodel@
logl 0 sex_l lage21  ur_rur obl_mn   @
line 1 dgy@
logl 1 ex2 @
fit@ null@

!linear quadratic dose-response 
line 1 +dgysq @ 
fit @ lrt @

!linear exponential dose-response
line 1 -dgysq@
logl 1 +dgy @
fit@  lrt@

!Final model Figure 3 <4 Gy
nomodel@
logl 0 sex_l lage21  ur_rur obl_mn   @
line 1 dgy@
logl 1 ex2 @
fit@ 
