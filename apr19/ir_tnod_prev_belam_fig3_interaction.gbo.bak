select largenod ne 1 and used_beh and dgy<4@
cases casesb @
sum casesb@

nomodel @
logl 0 study*sex_l study*lage21 study*ur_rur study*obl_mn  @ 
logl 1 ex2 @ 
line 1 dgy @ 
fit @ null@

line 1 study*dgy@ 
fit@ lrt@ null@

logl 1 study*ex2@  
fit@ lrt@